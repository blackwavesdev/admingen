{
  "name": "@blackwaves/admingen",
  "version": "0.0.1-beta.23",
  "private": false,
  "description": "Headless Admin Panel Framework for Elysia and Drizzle",
  "keywords": [
    "admingen",
    "elysia",
    "drizzle",
    "admin"
  ],
  "repository": {
  "type": "git",
  "url": "https://github.com/blackwavesdev/admingen.git",
  "directory": "packages/admingen"
},
  "license": "MIT",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [ "dist", "ui-assets" ],
"scripts": {
  "build": "bun run build:ui && bun run build:ts",
  "build:ts": "rm -rf dist && tsc -b",
"build:ui": "cd ../ui && bun run clean && bun run build && cd ../admingen && rm -rf ./ui-assets && mv ../ui/dist ./ui-assets",
  "dev": "tsc -b --watch"
},
  "dependencies": {
    "@elysiajs/static": "latest",
    "@blackwaves/admingen-types": "0.0.1-beta.23"
  },
  "peerDependencies": {
    "elysia": "*"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.3.0",
    "elysia": "latest"
  }
}